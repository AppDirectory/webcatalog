{
  "name": "WebCatalog",
  "version": "4.1.0",
  "description": "WebCatalog, Run any web app natively.",
  "main": "app/index.js",
  "scripts": {
    "postinstall": "install-app-deps && node src/main/libs/adblock/updateEasyList.js",
    "start": "yarn start.store",
    "start.dev": "yarn start.store.dev",
    "start.store.dev": "electron . --development=true",
    "start.store": "electron .",
    "start.ssb.dev": "electron . --development=true --name=\"Hello\" --url=\"http://hello.com\" --id=\"1490458685204\"",
    "start.ssb": "electron . --id=test --url=http://ads-blocker.com/testing/ --name=Shaka",
    "dev.main": "babel src/main -d app --copy-files --watch",
    "dev.ui": "cross-env NODE_ENV=development webpack -d --watch",
    "dev": "concurrently --kill-others \"yarn dev.main\" \"yarn dev.ui\"",
    "compile.main": "babili src/main -d app --copy-files",
    "compile.ui": "cross-env NODE_ENV=production webpack -p",
    "compile": "yarn compile.main && yarn compile.ui",
    "package": "node ./packager.js",
    "lint": "eslint ./src --ext .js --cache",
    "dist": "yarn compile && yarn package",
    "test": "mocha ./tests"
  },
  "repository": "https://github.com/webcatalog/webcatalog",
  "author": {
    "name": "Quang Lam",
    "email": "quang.lam2807@gmail.com",
    "url": "https://quanglam.me"
  },
  "license": "MIT",
  "devDependencies": {
    "@blueprintjs/core": "^1.14.0",
    "algoliasearch": "^3.22.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babili": "^0.0.12",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.16",
    "concurrently": "^3.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^4.0.0",
    "electron": "^1.6.2",
    "electron-builder": "^16.7.1",
    "electron-builder-squirrel-windows": "^16.7.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "immutable": "^3.8.1",
    "lodash.camelcase": "^4.3.0",
    "mocha": "^3.2.0",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-electron-web-view": "^2.0.1",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-batched-actions": "^0.1.5",
    "redux-thunk": "^2.1.0",
    "spectron": "^3.6.1",
    "webpack": "^2.3.3",
    "webpack-merge": "^4.0.0"
  },
  "dependencies": {
    "about-window": "^1.6.1",
    "ad-block": "https://github.com/quanglam2807/ad-block#2b97c56",
    "deep-equal": "^1.0.1",
    "electron-settings": "^3.0.14",
    "electron-updater": "^1.11.2",
    "electron-widevinecdm": "^1.0.2",
    "follow-redirects": "^1.2.3",
    "jsonfile": "^2.4.0",
    "lodash.camelcase": "^4.3.0",
    "mkdirp": "^0.5.1",
    "semver": "^5.3.0",
    "tmp": "^0.0.31",
    "yargs-parser": "^5.0.0"
  },
  "optionalDependencies": {
    "windows-shortcuts": "^0.1.4"
  }
}
