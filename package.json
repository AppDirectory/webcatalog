{
  "name": "WebCatalog",
  "version": "6.0.0",
  "description": "WebCatalog is an app store with thousands of exclusive apps for your Mac and PC.",
  "main": "app/index.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "yarn start.store",
    "start.dev": "yarn start.store.dev",
    "start.store.dev": "electron . --development=true",
    "start.store": "electron .",
    "start.shell.dev": "electron . --development=true --name=\"Hello\" --url=\"https://messenger.com\" --id=messenger",
    "start.shell": "electron . --id=facebook-messenger --url=https://messenger.com/ --name=Shaka",
    "dev.main": "babel src/main -d app --copy-files --watch",
    "dev.ui": "cross-env NODE_ENV=development node scripts/dev-ui.js",
    "dev": "concurrently --kill-others \"yarn dev.main\" \"yarn dev.ui\"",
    "build.main": "babili src/main -d app --copy-files",
    "build.ui": "cross-env NODE_ENV=production node scripts/build-ui.js",
    "build": "yarn build.main && yarn build.ui",
    "package": "node ./packager.js",
    "lint": "eslint ./src --ext .js",
    "dist": "yarn build && yarn package",
    "test": "mocha ./tests"
  },
  "repository": "https://github.com/webcatalog/webcatalog",
  "author": {
    "name": "Quang Lam",
    "email": "quang.lam2807@gmail.com",
    "url": "https://quanglam.me"
  },
  "license": "MIT",
