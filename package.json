    "chai": "^4.0.2",
    "chai-as-promised": "^6.0.0",
    "chalk": "^1.1.3",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.16",
    "concurrently": "^3.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
